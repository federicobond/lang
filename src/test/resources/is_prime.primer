def is_prime(n) {
    if n <= 3 {
        return n > 1
    } else {
        if n % 2 == 0 || n % 3 == 0 {
            return false
        } else {
            var i = 5
            while i * i <= n {
                if n % i == 0 || n % (i + 2) == 0 {
                    return false
                }
                i = i + 6
            }
            return true
        }
    }
}

var i = 1
while i <= 10 {
    if is_prime(i) {
        println(concat(i, " is prime."))
    } else {
        println(concat(i, " is not prime."))
    }
    i = i + 1
}
